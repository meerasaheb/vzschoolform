<p id="notice"><%= notice %></p>

<h1>Schooldetails</h1>

<table>
  <thead>
    <tr>
      <th>Schoolname</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Pincode</th>
      <th>Affiliatedboard</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schooldetails.each do |schooldetail| %>
      <tr>
        <td><%= schooldetail.schoolname %></td>
        <td><%= schooldetail.address %></td>
        <td><%= schooldetail.city %></td>
        <td><%= schooldetail.state %></td>
        <td><%= schooldetail.pincode %></td>
        <td><%= schooldetail.affiliatedboard %></td>
        <td><%= link_to 'Show', schooldetail %></td>
        <td><%= link_to 'Edit', edit_schooldetail_path(schooldetail) %></td>
        <td><%= link_to 'Destroy', schooldetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schooldetail', new_schooldetail_path %>
