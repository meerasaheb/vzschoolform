<p id="notice"><%= notice %></p>

<h1>Subsrcibingplans</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Features</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subsrcibingplans.each do |subsrcibingplan| %>
      <tr>
        <td><%= subsrcibingplan.subject %></td>
        <td><%= subsrcibingplan.features %></td>
        <td><%= link_to 'Show', subsrcibingplan %></td>
        <td><%= link_to 'Edit', edit_subsrcibingplan_path(subsrcibingplan) %></td>
        <td><%= link_to 'Destroy', subsrcibingplan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

